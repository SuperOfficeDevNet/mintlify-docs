                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/SuperOffice.CRM.Services.DictionaryStepInformation"
                                    }
                                }
                            },
                            "application/merge-patch+json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/SuperOffice.CRM.Services.DictionaryStepInformation"
                                    }
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "SuperOffice_Password": [],
                        "SuperId_Authentication": []
                    }
                ]
            }
